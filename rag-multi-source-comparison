      "cell_type": "code",
      "source": [
        "index = build_index_from_folder(DOCS_FOLDER)\n",
        "\n",
        "def argue_back(index: RagIndex, claim: str, k: int = 6) -> str:\n",
        "    pro_q  = f\"Evidence supporting: {claim}\"\n",
        "    con_q  = f\"Evidence opposing or contradicting: {claim}\"\n",
        "\n",
        "    pro_hits = index.search(pro_q, k=k)\n",
        "    con_hits = index.search(con_q, k=k)\n",
        "\n",
        "    pro_ctx = format_context(pro_hits, max_chars=1400)\n",
        "    con_ctx = format_context(con_hits, max_chars=1400)\n",
        "\n",
        "    prompt = f\"\"\"You are a skeptical assistant.\n",
        "Use ONLY the provided context. Produce:\n",
        "1) Best support for the claim (with citations to sources/chunks)\n",
        "2) Best counterargument (with citations)\n",
        "3) Your conclusion (mention uncertainty if needed)\n",
        "\n",
        "\n",
        "Rule: If the context doesn't have information about the query, just say 'The retrieved documents doens't have an answer for your query.'.\n",
        "\n",
        "Claim: {claim}\n",
        "\n",
        "SUPPORTING CONTEXT:\n",
        "{pro_ctx}\n",
        "\n",
        "OPPOSING CONTEXT:\n",
        "{con_ctx}\n",
        "\n",
        "Write the response clearly. Cite using [source=...|chunk=...].\"\"\"\n",
        "    return generate_answer(prompt, max_new_tokens=350)\n",
        "\n",
        "print(argue_back(index, \"Our refund policy allows refunds after 30 days.\"))\n"
      ],
      "metadata": {
        "id": "vIsqDj4DEIGF"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "index = build_index_from_folder(DOCS_FOLDER)\n",
        "\n",
        "# simple usage + decay state\n",
        "usage = {c.chunk_id: 0 for c in index.chunks}\n",
        "last_seen = {c.chunk_id: time.time() for c in index.chunks}\n",
        "\n",
        "def decayed_search(index: RagIndex, query: str, k: int = 5, half_life_hours: float = 24.0):\n",
        "    raw = index.search(query, k=30)  # get more candidates first\n",
        "    now = time.time()\n",
        "    scored = []\n",
        "    for sim, ch in raw:\n",
        "        age_hours = (now - last_seen[ch.chunk_id]) / 3600.0\n",
        "        decay = 0.5 ** (age_hours / half_life_hours)\n",
        "        bonus = 1.0 + 0.05 * usage[ch.chunk_id]\n",
        "        final = sim * decay * bonus\n",
        "        scored.append((final, sim, ch))\n",
        "    scored.sort(key=lambda x: x[0], reverse=True)\n",
        "    top = scored[:k]\n",
        "\n",
        "    # update usage for returned chunks\n",
        "    for _, _, ch in top:\n",
        "        usage[ch.chunk_id] += 1\n",
        "        last_seen[ch.chunk_id] = now\n",
        "    return top\n",
        "\n",
        "def rag_answer_forgetful(index: RagIndex, question: str) -> str:\n",
        "    hits = decayed_search(index, question, k=5)\n",
        "    ctx = \"\\n\".join([f\"[source={os.path.basename(ch.source)}|chunk={ch.chunk_id}|sim={sim:.3f}|rank={final:.3f}]\\n{ch.text}\"\n",
        "                     for (final, sim, ch) in hits])\n",
        "    prompt = f\"\"\"Answer using ONLY the context.\n",
        "Rule: If the context doesn't have information about the query, just say 'The retrieved documents doens't have an answer for your query.'.\n",
        "\n",
        "Question: {question}\n",
        "\n",
        "Context:\n",
        "{ctx}\n",
        "\n",
        "Answer:\"\"\"\n",
        "    return generate_answer(prompt, max_new_tokens=250)\n",
        "\n",
        "print(rag_answer_forgetful(index, \"What is the leave policy?\"))\n",
        "print(rag_answer_forgetful(index, \"What is the leave policy?\"))\n",
        "print(rag_answer_forgetful(index, \"What is the work policy?\"))\n"
      ],
      "metadata": {
        "id": "M-s3mEGZEK-c"
      },
      "execution_count": null,
      "outputs": []
    },
