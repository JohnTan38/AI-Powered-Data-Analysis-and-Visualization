      "source": [
        "!pip -q install faiss-cpu sentence-transformers transformers accelerate pypdf rapidfuzz tiktoken"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "import os, re, json, math, time, random\n",
        "from dataclasses import dataclass\n",
        "from typing import List, Dict, Tuple, Optional\n",
        "\n",
        "import numpy as np\n",
        "import faiss\n",
        "\n",
        "from sentence_transformers import SentenceTransformer\n",
        "from transformers import AutoTokenizer, AutoModelForSeq2SeqLM\n",
        "\n",
        "# --------- Embeddings + Generator ----------\n",
        "EMBED_MODEL_NAME = \"sentence-transformers/all-MiniLM-L6-v2\"\n",
        "GEN_MODEL_NAME   = \"google/flan-t5-base\"  # small + works on CPU; faster on GPU\n",
        "\n",
        "embedder = SentenceTransformer(EMBED_MODEL_NAME)\n",
        "\n",
        "gen_tokenizer = AutoTokenizer.from_pretrained(GEN_MODEL_NAME)\n",
        "gen_model = AutoModelForSeq2SeqLM.from_pretrained(GEN_MODEL_NAME)\n",
        "\n",
        "def generate_answer(prompt: str, max_new_tokens: int = 256) -> str:\n",
        "    inputs = gen_tokenizer(prompt, return_tensors=\"pt\", truncation=True)\n",
        "    outputs = gen_model.generate(**inputs, max_new_tokens=max_new_tokens)\n",
        "    return gen_tokenizer.decode(outputs[0], skip_special_tokens=True)\n",
        "\n",
        "# --------- Text ingest ----------\n",
        "def read_txt(path: str) -> str:\n",
        "    with open(path, \"r\", encoding=\"utf-8\", errors=\"ignore\") as f:\n",
        "        return f.read()\n",
        "\n",
        "def read_pdf(path: str) -> str:\n",
        "    from pypdf import PdfReader\n",
        "    reader = PdfReader(path)\n",
        "    parts = []\n",
        "    for page in reader.pages:\n",
        "        parts.append(page.extract_text() or \"\")\n",
        "    return \"\\n\".join(parts)\n",
        "\n",
        "def load_documents_from_folder(folder: str) -> List[Dict]:\n",
        "    \"\"\"\n",
        "    Returns list of dicts: {id, source, text}\n",
        "    Supports .txt and .pdf\n",
        "    \"\"\"\n",
        "    docs = []\n",
        "    for root, _, files in os.walk(folder):\n",
        "        for fn in files:\n",
        "            p = os.path.join(root, fn)\n",
        "            if fn.lower().endswith(\".txt\"):\n",
        "                txt = read_txt(p)\n",
        "            elif fn.lower().endswith(\".pdf\"):\n",
        "                txt = read_pdf(p)\n",
        "            else:\n",
        "                continue\n",
        "            docs.append({\"id\": len(docs), \"source\": p, \"text\": txt})\n",
        "    return docs\n",
        "\n",
        "# --------- Chunking ----------\n",
        "def simple_chunk(text: str, chunk_size: int = 900, overlap: int = 150) -> List[str]:\n",
        "    text = re.sub(r\"\\s+\", \" \", text).strip()\n",
        "    if not text:\n",
        "        return []\n",
        "    chunks = []\n",
        "    i = 0\n",
        "    while i < len(text):\n",
        "        chunks.append(text[i:i+chunk_size])\n",
        "        i += max(1, chunk_size - overlap)\n",
        "    return chunks\n",
        "\n",
        "@dataclass\n",
        "class Chunk:\n",
        "    chunk_id: int\n",
        "    doc_id: int\n",
        "    source: str\n",
        "    text: str\n",
        "\n",
        "def make_chunks(docs: List[Dict], chunk_size=900, overlap=150) -> List[Chunk]:\n",
        "    chunks = []\n",
        "    for d in docs:\n",
        "        for t in simple_chunk(d[\"text\"], chunk_size=chunk_size, overlap=overlap):\n",
        "            chunks.append(Chunk(\n",
        "                chunk_id=len(chunks),\n",
        "                doc_id=d[\"id\"],\n",
        "                source=d[\"source\"],\n",
        "                text=t\n",
        "            ))\n",
        "    return chunks\n",
        "\n",
        "# --------- FAISS index ----------\n",
        "class RagIndex:\n",
        "    def __init__(self, chunks: List[Chunk]):\n",
        "        self.chunks = chunks\n",
        "        self.texts = [c.text for c in chunks]\n",
        "        self.meta  = [{\"chunk_id\": c.chunk_id, \"doc_id\": c.doc_id, \"source\": c.source} for c in chunks]\n",
        "\n",
        "        emb = embedder.encode(self.texts, convert_to_numpy=True, show_progress_bar=True)\n",
        "        self.emb = emb.astype(\"float32\")\n",
        "        self.dim = self.emb.shape[1]\n",
        "\n",
        "        self.index = faiss.IndexFlatIP(self.dim)\n",
        "        faiss.normalize_L2(self.emb)\n",
        "        self.index.add(self.emb)\n",
        "\n",
        "    def search(self, query: str, k: int = 5) -> List[Tuple[float, Chunk]]:\n",
        "        q = embedder.encode([query], convert_to_numpy=True).astype(\"float32\")\n",
        "        faiss.normalize_L2(q)\n",
        "        scores, ids = self.index.search(q, k)\n",
        "        results = []\n",
        "        for s, idx in zip(scores[0], ids[0]):\n",
        "            if idx == -1:\n",
        "                continue\n",
        "            results.append((float(s), self.chunks[int(idx)]))\n",
        "        return results\n",
        "\n",
        "def format_context(results: List[Tuple[float, Chunk]], max_chars: int = 2200) -> str:\n",
        "    blocks = []\n",
        "    used = 0\n",
        "    for score, ch in results:\n",
        "        block = f\"[source={os.path.basename(ch.source)} | chunk={ch.chunk_id} | score={score:.3f}]\\n{ch.text}\\n\"\n",
        "        if used + len(block) > max_chars:\n",
        "            break\n",
        "        blocks.append(block)\n",
        "        used += len(block)\n",
        "    return \"\\n\".join(blocks).strip()\n",
        "\n",
        "def rag_answer(index: RagIndex, question: str, k: int = 5) -> Dict:\n",
        "    hits = index.search(question, k=k)\n",
        "    context = format_context(hits)\n",
        "\n",
        "    prompt = f\"\"\"Answer the question using ONLY the context.\n",
        "If the context doesn't contain the answer, say: \"I don't know from the provided documents.\"\n",
        "\n",
        "Question: {question}\n",
        "\n",
        "Context:\n",
        "{context}\n",
        "\n",
        "Answer:\"\"\"\n",
        "    ans = generate_answer(prompt)\n",
        "    return {\"question\": question, \"answer\": ans, \"hits\": hits, \"context\": context}\n",
        "\n",
        "# --------- Quick start helper ----------\n",
        "def build_index_from_folder(folder: str) -> RagIndex:\n",
        "    docs = load_documents_from_folder(folder)\n",
        "    chunks = make_chunks(docs)\n",
        "    print(f\"Loaded {len(docs)} docs -> {len(chunks)} chunks\")\n",
        "    return RagIndex(chunks)\n"
      ],
      "metadata": {
